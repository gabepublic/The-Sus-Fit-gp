      /* istanbul ignore next */
      cov_nt0vdori3().s[101]++;
      await new Promise((resolve, reject) => {
        /* istanbul ignore next */
        cov_nt0vdori3().f[33]++;
        cov_nt0vdori3().s[102]++;
        img.onload = () => {
          /* istanbul ignore next */
          cov_nt0vdori3().f[34]++;
          cov_nt0vdori3().s[103]++;
          return resolve();
        };
        /* istanbul ignore next */
        cov_nt0vdori3().s[104]++;
        img.onerror = () => {
          /* istanbul ignore next */
          cov_nt0vdori3().f[35]++;
          cov_nt0vdori3().s[105]++;
