  /* istanbul ignore next */
  (cov_nt0vdori3().s[113]++, (0, _react.useCallback)((image, x =
  /* istanbul ignore next */
  (cov_nt0vdori3().b[11][0]++, 0), y =
  /* istanbul ignore next */
