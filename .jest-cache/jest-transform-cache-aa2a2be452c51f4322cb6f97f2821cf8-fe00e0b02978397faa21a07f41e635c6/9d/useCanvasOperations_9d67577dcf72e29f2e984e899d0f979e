function useCanvasImageLoader(canvas) {
  /* istanbul ignore next */
  cov_nt0vdori3().f[31]++;
  const [isLoading, setIsLoading] =
  /* istanbul ignore next */
  (cov_nt0vdori3().s[91]++, (0, _react.useState)(false));
  const [error, setError] =
  /* istanbul ignore next */
  (cov_nt0vdori3().s[92]++, (0, _react.useState)(null));
