      width: newWidth,
      height: newHeight
    });
  }, [canvas, maintainContent, utils]));
  const autoResize =
  /* istanbul ignore next */
  (cov_nt0vdori3().s[130]++, (0, _react.useCallback)(container => {
    /* istanbul ignore next */
    cov_nt0vdori3().f[40]++;
    const resizeObserver =
    /* istanbul ignore next */
    (cov_nt0vdori3().s[131]++, new ResizeObserver(entries => {
      /* istanbul ignore next */
      cov_nt0vdori3().f[41]++;
