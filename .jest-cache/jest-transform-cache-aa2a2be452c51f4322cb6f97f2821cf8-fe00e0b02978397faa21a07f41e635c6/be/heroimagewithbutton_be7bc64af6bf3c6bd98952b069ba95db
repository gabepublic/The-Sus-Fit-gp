        //     renderedImageHeight,
        //     category: renderedImageHeight < 500 ? 'SMALL' : renderedImageHeight < 600 ? 'MEDIUM' : 'LARGE',
        //     baseLeftPercent,
        //     positionAdjustment,
        //     newLeftPercent: newLeftPercent.toFixed(2),
        //     buttonScale: scale,
        //     finalScale: (1.5 * scale).toFixed(2)
        // })
        /* istanbul ignore next */
        cov_mtm2vnmap().s[36]++;
        return;
      } else
      /* istanbul ignore next */
      {
        cov_mtm2vnmap().b[7][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_mtm2vnmap().b[5][1]++;
    }
    // Fallback: screen width
    cov_mtm2vnmap().s[37]++;
    if (typeof window !== 'undefined') {
      /* istanbul ignore next */
      cov_mtm2vnmap().b[10][0]++;
      const screenWidth =
      /* istanbul ignore next */
      (cov_mtm2vnmap().s[38]++, window.innerWidth);
      let positionAdjustment =
      /* istanbul ignore next */
      (cov_mtm2vnmap().s[39]++, 0);
      let scale =
      /* istanbul ignore next */
      (cov_mtm2vnmap().s[40]++, 1);
      /* istanbul ignore next */
      cov_mtm2vnmap().s[41]++;
      if (screenWidth < 1400) {
        /* istanbul ignore next */
        cov_mtm2vnmap().b[11][0]++;
        cov_mtm2vnmap().s[42]++;
        positionAdjustment = 6.0;
        /* istanbul ignore next */
        cov_mtm2vnmap().s[43]++;
        scale = 0.67;
      } else {
        /* istanbul ignore next */
        cov_mtm2vnmap().b[11][1]++;
        cov_mtm2vnmap().s[44]++;
        if (screenWidth < 1800) {
          /* istanbul ignore next */
          cov_mtm2vnmap().b[12][0]++;
          cov_mtm2vnmap().s[45]++;
          positionAdjustment = -3.0 // Desktop adjustment
          ;
          /* istanbul ignore next */
          cov_mtm2vnmap().s[46]++;
          scale = 0.8;
        } else
        /* istanbul ignore next */
        {
          cov_mtm2vnmap().b[12][1]++;
        }
      }
      const newLeftPercent =
      /* istanbul ignore next */
      (cov_mtm2vnmap().s[47]++, baseLeftPercent + positionAdjustment);
      /* istanbul ignore next */
      cov_mtm2vnmap().s[48]++;
      setAdjustedLeftPercent(`${newLeftPercent}%`);
      /* istanbul ignore next */
      cov_mtm2vnmap().s[49]++;
      setButtonScale(scale);
    } else
    /* istanbul ignore next */
    {
      cov_mtm2vnmap().b[10][1]++;
    }
  }, [overlayButton]));
  // Calculate position when image loads
  /* istanbul ignore next */
  cov_mtm2vnmap().s[50]++;
  const handleImageLoad = () => {
    /* istanbul ignore next */
    cov_mtm2vnmap().f[4]++;
    cov_mtm2vnmap().s[51]++;
    setTimeout(() => {
      /* istanbul ignore next */
      cov_mtm2vnmap().f[5]++;
      cov_mtm2vnmap().s[52]++;
      calculateButtonPositionAndSize();
    }, 250);
  };
  // Initial calculation on mount
  /* istanbul ignore next */
  cov_mtm2vnmap().s[53]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_mtm2vnmap().f[6]++;
    cov_mtm2vnmap().s[54]++;
    calculateButtonPositionAndSize();
    const timeoutId =
    /* istanbul ignore next */
    (cov_mtm2vnmap().s[55]++, setTimeout(calculateButtonPositionAndSize, 100));
    /* istanbul ignore next */
    cov_mtm2vnmap().s[56]++;
    return () => {
      /* istanbul ignore next */
      cov_mtm2vnmap().f[7]++;
      cov_mtm2vnmap().s[57]++;
      return clearTimeout(timeoutId);
    };
  }, [overlayButton, calculateButtonPositionAndSize]);
  // Recalculate on window resize
  /* istanbul ignore next */
  cov_mtm2vnmap().s[58]++;
  (0, _react.useEffect)(() => {
    /* istanbul ignore next */
    cov_mtm2vnmap().f[8]++;
    cov_mtm2vnmap().s[59]++;
    const handleResize = () => {
      /* istanbul ignore next */
      cov_mtm2vnmap().f[9]++;
      cov_mtm2vnmap().s[60]++;
      calculateButtonPositionAndSize();
    };
    /* istanbul ignore next */
    cov_mtm2vnmap().s[61]++;
    window.addEventListener('resize', handleResize);
    /* istanbul ignore next */
    cov_mtm2vnmap().s[62]++;
    return () => {
      /* istanbul ignore next */
      cov_mtm2vnmap().f[10]++;
      cov_mtm2vnmap().s[63]++;
      return window.removeEventListener('resize', handleResize);
    };
  }, [overlayButton, calculateButtonPositionAndSize]);
  // Always render button when overlayButton is provided, but apply disabled state
  const shouldShowButton =
  /* istanbul ignore next */
  (cov_mtm2vnmap().s[64]++,
  /* istanbul ignore next */
  (cov_mtm2vnmap().b[13][0]++, overlayButton) &&
  /* istanbul ignore next */
  (cov_mtm2vnmap().b[13][1]++, isButtonReady));
  /* istanbul ignore next */
  cov_mtm2vnmap().s[65]++;
  return /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
    className: (0, _utils.cn)("relative w-full flex items-center justify-center", "h-[50vh] min-h-[400px] max-h-[800px]", className),
    children: /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
      ref: containerRef,
      className: "relative w-full h-full max-w-5xl",
      children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_image.default, {
        ref: imageRef,
        src: src,
        alt: alt,
        fill: true,
        priority: priority,
        className: "object-contain drop-shadow-2xl",
        sizes: "(max-width: 768px) 100vw, (max-width: 1200px) 90vw, 80vw",
        onLoad: handleImageLoad
      }),
      /* istanbul ignore next */
      (cov_mtm2vnmap().b[14][0]++, shouldShowButton) &&
      /* istanbul ignore next */
      (cov_mtm2vnmap().b[14][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
        className: "absolute inset-0",
        style: {
          transform: 'scale(1.5)',
          transformOrigin: 'center center'
        },
        children: /*#__PURE__*/(0, _jsxruntime.jsx)("button", {
          onClick: overlayButton.onClick,
          disabled: overlayButton.disabled,
          "data-test": "generate-button",
          className: (0, _utils.cn)("absolute z-[200] rounded-full pointer-events-auto", "transition-all duration-150 ease-in-out", overlayButton.disabled ?
          /* istanbul ignore next */
          (cov_mtm2vnmap().b[15][0]++, "cursor-not-allowed opacity-50") :
          /* istanbul ignore next */
          (cov_mtm2vnmap().b[15][1]++, "hover:scale-110 active:scale-95 cursor-pointer"), buttonSizes[
          /* istanbul ignore next */
          (cov_mtm2vnmap().b[16][0]++, overlayButton.size) ||
          /* istanbul ignore next */
          (cov_mtm2vnmap().b[16][1]++, 'md')], overlayButton.className),
          style: {
            left: adjustedLeftPercent,
            top: overlayButton.position.topPercent,
            transform: `translate(-50%, -50%) scale(${buttonScale})`,
            background: `
                                    radial-gradient(circle at 35% 35%, rgba(255, 255, 255, 0.6) 0%, rgba(255, 255, 255, 0) 60%),
                                    #D80E0E
                                `,
            border: '2px solid #BF1212',
            boxShadow: `
                                    0 15px 25px rgba(0, 0, 0, 0.4),
                                    0 0 0 4px rgba(240, 228, 228, 0.5)
                                `
          },
          "aria-label": "Camera capture button"
        })
      }))]
    })
  });
}
