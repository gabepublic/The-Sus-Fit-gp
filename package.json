{
  "name": "The Sus Fit",
  "description": "Branding campaign",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "docs": "typedoc",
    "docs:serve": "npx http-server docs -p 8080 -o",
    "docs:stop": "lsof -ti:8080 | xargs kill -9 2>/dev/null || echo 'No process running on port 8080'",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:ci": "jest --ci --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "test:agent": "jest --ci --runInBand --silent",
    "test:unit": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:enforce": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "test:failures": "jest --silent --verbose=false --coverage=false --passWithNoTests",
    "test:e2e": "playwright test"
  },
